def search_list(a, x):
    n = len(a)
    for i in range(0, n): # 리스트 a의 모든 값을 차례로
        if x == a[i]: # x 값과 비교하여
            return i # 같으면 위치를 돌려준다.
    return -1 # 끝까지 비교해서 없으면 -1을 돌려준다.

v = [17, 92, 18, 33, 58, 7, 33, 42]
print(search_list(v, 18))
print(search_list(v, 33))
print(search_list(v, 900))
